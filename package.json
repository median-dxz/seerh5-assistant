  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/eslint__js": "^8.42.3",
    "@vitest/coverage-v8": "2.0.3",
    "concurrently": "^8.2.2",
    "eslint": "^9.7.0",
    "eslint-plugin-react-hooks": "5.1.0-rc-8971381549-20240625",
    "eslint-plugin-react-refresh": "^0.4.8",
    "globals": "^15.8.0",
    "husky": "^9.1.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "8.0.0-alpha.34",
    "vite": "^5.3.4",
    "vitest": "2.0.3"
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "lint-staged": {
    "*.{ts,tsx,json,css,md}": "prettier --write"
