  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "@vitest/coverage-v8": "2.0.3",
    "commander": "^12.1.0",
    "concurrently": "^8.2.2",
